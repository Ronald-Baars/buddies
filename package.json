{
  "name": "buddies.baars.design",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "scripts": {
    "client": "react-scripts start",
    "server": "node server",
    "start": "concurrently -k -n \"server,client\" -c \"blue,green\" \"npm run server\" \"npm run client\"",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-watch": "^7.0.0"
  }
}
